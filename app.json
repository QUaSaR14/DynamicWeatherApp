{
  "expo": {
    "name": "WeatherApp",
    "slug": "weatherapp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hot.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.quasar.weatherapp"
    },
    "description": "Shows real time weather using device location",
    "githubUrl": "https://github.com/QUaSaR14/DynamicWeatherApp"
  }
}
